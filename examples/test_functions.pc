# Test user-defined functions with parameters and return values

def add(a, b):
    return a + b

def multiply(x, y):
    result = x * y
    return result

def greet(name):
    print(name)
    return 42

# Test function calls
sum_result = add(10, 20)
print(sum_result)

mult_result = multiply(5, 6)
print(mult_result)

greeting_result = greet(999)
print(greeting_result)

# Test nested calculations
final = add(mult_result, 100)
print(final)

print(888)
