# PC Language - Complete Feature Test

print(999)

# Variables and arithmetic
x = 10
y = 20
z = x + y
print(z)

# Control flow - if
if x > 5:
    print(100)
else:
    print(200)

# For loop
total = 0
for i in range(5):
    total = total + i
print(total)

# PWN functions
addr = 4194304
hex_addr = hex(addr)
print(hex_addr)

# Type conversion
value = int(123)
print(value)

# String length
packed = p64(3735928559)
print(len(packed))

print(777)
